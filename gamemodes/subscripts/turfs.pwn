stock GetZoneName(playerid)
{
	new str[60],Float:x,Float:y,Float:z;
	GetPlayerPos(playerid,x,y,z);
	str="Unknown";
 	for(new i = 0; i != 354; i++ )
 	{
		if(x >= Turfs[i][turf_pos][0] && x <= Turfs[i][turf_pos][2] && y >= Turfs[i][turf_pos][1] && y <= Turfs[i][turf_pos][3])
		{
			 format(str,sizeof(str),"%s",Turfs[i][turf_name]);
			 break;
		}
	}
	return str;
}

new TurfID[sizeof(Turfs)];
new TurfTick[sizeof(Turfs)];
new TurfArea[sizeof(Turfs)];

enum gwenum
{
  bool:gwON,
  gang1,
