  gang2kills
}
new TurfGW[sizeof(Turfs)][gwenum];

stock bool:IsPlayerInGangZone(playerid, zoneid)
{
    new Float:x,Float:y,Float:z;
    GetPlayerPos(playerid, x, y, z);
    if((Turfs[ zoneid ][ turf_pos ][0] < x) && (Turfs[ zoneid ][ turf_pos ][2] > x) && (Turfs[ zoneid ][ turf_pos ][1] < y) && (Turfs[ zoneid ][ turf_pos ][3] > y)) return true;
    else return false;
}
stock GetPlayerTurf(playerid)
{
  new t=-255;
  for(new x=0; x<sizeof(Turfs); x++)
  {
	 if(IsPlayerInGangZone(playerid,x))
	 {
		 t=x;
		 break;
	 }
  }

  return t;
}
